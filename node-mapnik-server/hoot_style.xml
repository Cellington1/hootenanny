<?xml version="1.0" encoding="utf-8"?>
 <Map background-color="#00000000" srs="+proj=latlong +datum=WGS84">
	<Style name="hoot">
		<Rule>
			<Filter>[building] &lt;&gt; '' or [name] &lt;&gt; ''</Filter>
			<LineSymbolizer stroke="rgb(255, 85, 153)" stroke-width="2" stroke-linejoin="miter"
				stroke-linecap="square" />
			<PolygonSymbolizer fill="rgb(255, 85, 153)" fill-opacity="0.3" />
		</Rule>
		<Rule>
			<Filter>[highway] &lt;&gt; '' or [name] &lt;&gt; ''</Filter>
			<LineSymbolizer stroke="rgb(255, 85, 153)" stroke-width="2" stroke-linejoin="round"
				stroke-linecap="round" />
		</Rule>
		<Rule>
			<Filter>[amenity] &lt;&gt; '' or [name] &lt;&gt; ''</Filter>
			<MarkersSymbolizer file="poi.svg" fill="rgb(255, 85, 153)" />
		</Rule>
	</Style>
	<Layer name="hoot" status="on" srs="+proj=latlong +datum=WGS84">
		<StyleName>hoot</StyleName>
		<Datasource>
			<Parameter name="type">osm</Parameter>
			<Parameter name="file">PortAuPrinceOsmPoiRoadBuilding.osm</Parameter>
		</Datasource>
	</Layer>
 </Map>
