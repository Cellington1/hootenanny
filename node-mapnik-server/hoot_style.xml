<?xml version="1.0" encoding="utf-8"?>
 <Map background-color="#00000000" srs="+init=epsg:3857">
	<Style name="polygon">
		<Rule>
			<Filter>[building] &lt;&gt; ''</Filter>
			<LineSymbolizer stroke="${COLOR}" stroke-width="2" stroke-linejoin="miter"
				stroke-linecap="square" />
			<PolygonSymbolizer fill="${COLOR}" fill-opacity="0.3" />
		</Rule>
	</Style>
	<Style name="line">
		<Rule>
			<Filter>[highway] &lt;&gt; ''</Filter>
			<LineSymbolizer stroke="${COLOR}" stroke-width="2" stroke-linejoin="round"
				stroke-linecap="round" />
		</Rule>
	</Style>
	<Style name="point">
		<Rule>
			<Filter>[amenity] &lt;&gt; ''</Filter>
			<MarkersSymbolizer file="poi.svg" fill="${COLOR}" />
		</Rule>
	</Style>
 </Map>
